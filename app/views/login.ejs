<%include header.ejs%><style type="text/css">    .input-group ,.btn-group{        margin-top: 20px;    }</style><div class="container">    <div class="col-md-6 col-md-offset-3">        <form action="/login" method="post">         <h3><%= title%></h3>        <div class="">        <div class="input-group col-md-12">            <input type="text" name="username" class="form-control" placeholder="Username" >        </div>        <div class="input-group col-md-12">            <input type="text" name="password" class="form-control" placeholder="passWord" >            <%if(msg.length){%>            <label class="text-warning"><%=msg%></label>            <%}%>        </div>        </div>        <div class="btn-group col-md-12 clearfix" role="group">            <button type="submit" class="btn btn-default pull-right"><%= title%></button>        </div>        </form>    </div></div><%include footer.ejs%>